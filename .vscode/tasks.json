{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build",
            "command": "gcc",
            "args": [
                "-g",
                "-Wall",
                "${fileDirname}\\main.c",
                "${fileDirname}\\init.c",
                "${fileDirname}\\process_controler.c",
                "${fileDirname}\\resource_controler.c",
                "${fileDirname}\\test_shell.c",
                "${fileDirname}\\timeout_and_scheduler.c",
                "-o",
                "${fileDirname}\\main.exe",
            ],
            "type": "shell",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
        }
    ]
}